#include <PRIZM.h>
PRIZM Jagmeet;
void setup() {
Jagmeet.PrizmBegin();
Jagmeet.setMotorInvert(1,1);

}

void loop() {
Jagmeet.setMotorSpeeds(360,360);
  delay(4000);
  Jagmeet.setMotorSpeeds(360,-360);
  delay(500);
  Jagmeet.setMotorSpeeds(360,360);
  delay(1250);
  Jagmeet.setMotorSpeeds(-360,360);
  delay(500);
  Jagmeet.setMotorSpeeds(360,360);
  delay(1600);
  Jagmeet.setCRServoState(1, 100);
  delay(500);
  Jagmeet.setCRServoState(1, -100);
  delay(100);
  Jagmeet.setCRServoState(1, 100);
  delay(100);
  Jagmeet.setCRServoState(1, -100);
  delay(100);
  Jagmeet.setCRServoState(1, 100);
  delay(100);
  Jagmeet.setCRServoState(1, -100);
  delay(100);
  Jagmeet.setCRServoState(1, 100);
  delay(100);
  Jagmeet.setCRServoState(1, -100);
  delay(100);
  Jagmeet.setCRServoState(1, 100);
  delay(100);
  Jagmeet.setCRServoState(1, -100);
  delay(100);
  Jagmeet.setCRServoState(1, 100);
  delay(100);
  Jagmeet.setCRServoState(1, -100);
  delay(100);
  Jagmeet.setCRServoState(1, 0);
  Jagmeet.PrizmEnd();


}
